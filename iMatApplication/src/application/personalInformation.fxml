<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root prefHeight="698.0" prefWidth="649.0" stylesheets="@application.css" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <ScrollPane id="Register" fitToHeight="true" fitToWidth="true" stylesheets="@application.css" BorderPane.alignment="CENTER">
         <content>
            <BorderPane stylesheets="@application.css">
               <top>
                  <BorderPane BorderPane.alignment="CENTER">
                     <center>
                        <Label maxHeight="60.0" text="Var vänlig fyll i personuppgifter nedan för att gå vidare i betalningen." wrapText="true" BorderPane.alignment="CENTER">
                           <BorderPane.margin>
                              <Insets left="80.0" right="80.0" />
                           </BorderPane.margin>
                        </Label>
                     </center>
                     <top>
                        <Label text="Leveransadress" BorderPane.alignment="CENTER">
                           <font>
                              <Font size="36.0" />
                           </font>
                           <BorderPane.margin>
                              <Insets top="10.0" />
                           </BorderPane.margin>
                        </Label>
                     </top>
                     <bottom>
                        <VBox>
                           <children>
                              <Separator prefWidth="200.0" />
                              <HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <RadioButton fx:id="rBtnProfile" mnemonicParsing="false" onMouseReleased="#useProfile" text="Använd profilinställningar" />
                                    <RadioButton fx:id="rBtnOther" mnemonicParsing="false" onMouseReleased="#useOther" text="Leverera till annan adress" />
                                 </children>
                                 <padding>
                                    <Insets top="10.0" />
                                 </padding>
                              </HBox>
                           </children>
                           <BorderPane.margin>
                              <Insets />
                           </BorderPane.margin>
                           <padding>
                              <Insets top="10.0" />
                           </padding>
                        </VBox>
                     </bottom>
                     <padding>
                        <Insets bottom="10.0" />
                     </padding>
                  </BorderPane>
               </top>
               <center>
                  <HBox alignment="CENTER" BorderPane.alignment="CENTER">
                     <children>
                        <VBox alignment="TOP_RIGHT" spacing="10.0">
                           <children>
                              <Label maxHeight="39.0" minHeight="39.0" prefHeight="39.0" text="Förnamn:">
                                 <padding>
                                    <Insets right="5.0" />
                                 </padding>
                              </Label>
                              <Label maxHeight="39.0" minHeight="39.0" prefHeight="39.0" text="Efternamn:">
                                 <padding>
                                    <Insets right="5.0" />
                                 </padding>
                              </Label>
                              <Label maxHeight="39.0" minHeight="39.0" prefHeight="39.0" text="E-post:">
                                 <padding>
                                    <Insets right="5.0" />
                                 </padding>
                              </Label>
                              <Label maxHeight="39.0" minHeight="39.0" prefHeight="39.0" text="Adress:">
                                 <padding>
                                    <Insets right="5.0" />
                                 </padding>
                              </Label>
                              <Label maxHeight="39.0" minHeight="39.0" prefHeight="39.0" text="Telefon:">
                                 <padding>
                                    <Insets right="5.0" />
                                 </padding>
                              </Label>
                              <Label maxHeight="39.0" minHeight="39.0" prefHeight="39.0" text="Postkod:">
                                 <padding>
                                    <Insets right="5.0" />
                                 </padding>
                              </Label>
                              <Label maxHeight="39.0" minHeight="39.0" prefHeight="39.0" text="Leveransdatum:">
                                 <padding>
                                    <Insets right="5.0" />
                                 </padding>
                              </Label>
                           </children>
                        </VBox>
                        <VBox spacing="10.0">
                           <children>
                              <TextField fx:id="txtfPhone" promptText="Förnamn">
                                 <tooltip>
                                    <Tooltip text="Telefon" />
                                 </tooltip>
                              </TextField>
                              <TextField fx:id="txtfEmail" promptText="Efternamn">
                            	<tooltip>
                              	<Tooltip text="E-post" />
                           	</tooltip>
                              </TextField>
                              <TextField fx:id="txtfSurname" promptText="E-post">
                               	<tooltip>
                              		<Tooltip text="Förnamn" />
                          		</tooltip>
                              </TextField>
                              <TextField fx:id="txtfLastname" promptText="Adress">
                                  <tooltip>
                              		<Tooltip text="Efternamn" />
                           		</tooltip>
                              </TextField>
                              <TextField fx:id="txtfAdress" promptText="Telefon">
                               	 <tooltip>
                              		<Tooltip text="Adress" />
                          	 	</tooltip>
                              </TextField>
                              <HBox fillHeight="false" spacing="10.0">
                                 <children>
                                    <TextField fx:id="txtfPostcode" prefWidth="110.0" promptText="Postkod">
                                       <tooltip>
                                   		 <Tooltip text="Postkod" />
                                 		</tooltip>
                                    </TextField>
                                    <Label maxHeight="39.0" maxWidth="30.0" minHeight="39.0" minWidth="30.0" prefHeight="39.0" text="Ort:" />
                                    <TextField fx:id="txtfCity" prefHeight="39.0" prefWidth="270.0" promptText="Ort" />
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER_LEFT">
                                 <children>
                                    <ChoiceBox fx:id="choiceDay" prefWidth="150.0" />
                                    <ChoiceBox fx:id="choiceTime" prefHeight="39.0" prefWidth="270.0">
                                       <HBox.margin>
                                          <Insets left="10.0" />
                                       </HBox.margin>
                                    </ChoiceBox>
                                 </children>
                              </HBox>
                              <Label prefWidth="419.0" text="Väljer du inte kommer varorna levereras så fort som möjligt (vanligen inom 2 timmar)" wrapText="true">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </Label>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </center>
            </BorderPane>
         </content>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </ScrollPane>
   </center>
   <bottom>
      <Button fx:id="goToPaymentBtn" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#register" text="Gå vidare" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="10.0" left="50.0" right="50.0" top="10.0" />
         </padding>
         <BorderPane.margin>
            <Insets bottom="20.0" left="100.0" right="100.0" top="10.0" />
         </BorderPane.margin>
         <tooltip>
            <Tooltip text="Klicka för att gå vidare till nästa steg i köpet" />
         </tooltip>
      </Button>
   </bottom>
</fx:root>
