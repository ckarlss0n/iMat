<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.media.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" maxWidth="250.0" minWidth="250.0" prefWidth="250.0" styleClass="leftBar" stylesheets="@application.css" BorderPane.alignment="CENTER">
         <content>
            <BorderPane prefHeight="200.0" prefWidth="200.0">
               <top>
                  <BorderPane minHeight="54.0" prefHeight="54.0" styleClass="grayGradient" BorderPane.alignment="CENTER">
                     <center>
                        <TextField promptText="Sök..." BorderPane.alignment="CENTER">
                           <BorderPane.margin>
                              <Insets bottom="8.0" left="10.0" top="8.0" />
                           </BorderPane.margin>
                        </TextField>
                     </center>
                     <right>
                        <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" text="Sök" BorderPane.alignment="CENTER">
                           <BorderPane.margin>
                              <Insets bottom="8.0" right="10.0" top="8.0" />
                           </BorderPane.margin>
                           <graphic>
                              <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../icon16/white/search.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </right>
                  </BorderPane>
               </top>
               <center>
                  <Accordion fx:id="categoryAccordation" styleClass="redAccordion" stylesheets="@application.css" BorderPane.alignment="CENTER">
                    <panes>
                      <TitledPane animated="false" stylesheets="@application.css" text="untitled 1">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                        </content>
                      </TitledPane>
                      <TitledPane animated="false" text="untitled 2">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                        </content>
                      </TitledPane>
                      <TitledPane animated="false" disable="true">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                        </content>
                      </TitledPane>
                    </panes>
                  </Accordion>
               </center>
            </BorderPane>
         </content>
      </ScrollPane>
   </left>
   <right>
      <BorderPane fx:id="borderPane" maxWidth="250.0" minWidth="250.0" prefWidth="250.0" stylesheets="@application.css">
         <top>
            <Label alignment="CENTER" contentDisplay="RIGHT" styleClass="grayGradient" text="Kundvagn" textAlignment="CENTER" BorderPane.alignment="CENTER">
               <graphic>
                  <ImageView fitHeight="32.0" fitWidth="55.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icon32/cart.png" />
                     </image>
                  </ImageView>
               </graphic>
               <padding>
                  <Insets bottom="11.0" top="11.0" />
               </padding>
            </Label>
         </top>
         <bottom>
            <BorderPane BorderPane.alignment="CENTER">
               <bottom>
                  <Button contentDisplay="RIGHT" mnemonicParsing="false" onAction="#goToCheckOut" prefHeight="39.0" text="Gå till kassan" BorderPane.alignment="CENTER">
                     <graphic>
                        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../icon32/white/circle-right.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <font>
                        <Font size="18.0" />
                     </font>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
               </bottom>
               <center>
                  <Label alignment="CENTER" styleClass="grayGradient" text="35040 kr" textAlignment="CENTER" BorderPane.alignment="CENTER">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </center>
            </BorderPane>
         </bottom>
         <center>
            <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" vbarPolicy="ALWAYS" BorderPane.alignment="CENTER">
               <content>
                  <GridPane fx:id="gridPane">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                  </GridPane>
               </content>
            </ScrollPane>
         </center>
      </BorderPane>
   </right>
   <top>
      <BorderPane styleClass="topBar" stylesheets="@application.css" BorderPane.alignment="CENTER">
         <left>
            <Button fx:id="btnHome" mnemonicParsing="false" onAction="#goToHome" text="Hem" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="20.0" />
               </BorderPane.margin>
               <graphic>
                  <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icon32/white/home3.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </left>
         <center>
            <ImageView fitHeight="45.0" fitWidth="113.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <image>
                  <Image url="@../../iMatLogo.png" />
               </image>
               <BorderPane.margin>
                  <Insets bottom="20.0" top="20.0" />
               </BorderPane.margin>
               <effect>
                  <DropShadow blurType="ONE_PASS_BOX" color="#00000070" />
               </effect>
            </ImageView>
         </center>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <right>
            <Button fx:id="nextButton" mnemonicParsing="false" onAction="#goToMyProfile" text="Min profil" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets right="20.0" />
               </BorderPane.margin>
               <graphic>
                  <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icon32/white/user.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </right>
      </BorderPane>
   </top>
   <center>
      <BorderPane BorderPane.alignment="CENTER">
         <center>
            <StackPane fx:id="stackPane" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <BorderPane BorderPane.alignment="CENTER">
               <left>
                  <ToolBar minHeight="55.0" prefHeight="55.0" BorderPane.alignment="CENTER">
                    <items>
                      <Button mnemonicParsing="false" text="Varma drycker" />
                        <Button mnemonicParsing="false" text="Te" />
                    </items>
                  </ToolBar>
               </left>
               <center>
                  <ToolBar maxHeight="55.0" minHeight="55.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="55.0" stylesheets="@application.css" BorderPane.alignment="CENTER">
                     <items>
                        <ChoiceBox prefWidth="150.0" />
                        <ChoiceBox prefWidth="150.0" />
                     </items>
                  </ToolBar>
               </center>
            </BorderPane>
         </top>
      </BorderPane>
   </center>
</fx:root>
